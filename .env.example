# OpenAI API Key (Primary)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_api_key_here

# DeepSeek API Key (Fallback)
# Get your key from: https://platform.deepseek.com/api_keys
# If OPENAI_API_KEY is not set, DeepSeek will be used automatically
DEEPSEEK_API_KEY=your_deepseek_key_here

# Smallest.ai API Key (Voice - TTS/STT)
# Get your key from: https://console.smallest.ai
# Required for voice mode
SMALLEST_API_KEY=your_smallest_key_here
